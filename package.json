{
  "name": "front_web_mrmilu",
  "version": "0.0.0",
  "author": "Mr.Mil√∫",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "preconstruct watch --onSuccess='yarn workspace %pkg% yalc-push'",
    "dev": "preconstruct dev",
    "build": "preconstruct build",
    "patch": "patch-package",
    "postinstall": "yarn patch-package",
    "release": "standard-version",
    "generate-docs": "yarn workspaces run typedoc && node scripts/update_docusaurus_intro.js",
    "test": "jest",
    "test:badges": "jest --collect-coverage && make-coverage-badge",
    "skip-ci": "git commit --allow-empty -m \"chore(root): [skip ci]\""
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "7.17.10",
    "patch-package": "6.4.7",
    "typescript": "4.6.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.17.10",
    "@babel/preset-env": "7.17.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@commitlint/prompt-cli": "16.2.4",
    "@preconstruct/cli": "2.1.5",
    "@types/jest": "27.5.0",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "chalk": "4.1.2",
    "css-mediaquery": "0.1.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-chain": "1.1.5",
    "jest-environment-jsdom": "28.1.0",
    "jest-extended": "2.0.0",
    "lint-staged": "12.4.1",
    "make-coverage-badge": "1.2.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.6.2",
    "standard-version": "9.5.0",
    "ts-node": "10.7.0",
    "typedoc": "0.22.15",
    "typedoc-plugin-markdown": "3.12.1"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
