    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "gitpkg": "^1.0.0-beta.2",
    "husky": "7.0.4",
