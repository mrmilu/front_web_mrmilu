{
  "name": "front_web_mrmilu",
  "version": "0.0.0",
  "author": "Mr.Mil√∫",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "preconstruct watch --onSuccess='yarn workspace %pkg% yalc-push'",
    "dev": "preconstruct dev",
    "build": "preconstruct build",
    "patch": "patch-package",
    "postinstall": "yarn patch-package",
    "release": "commit-and-tag-version",
    "generate-docs": "yarn workspaces run typedoc && node scripts/update_docusaurus_intro.js",
    "test": "jest",
    "test:badges": "jest --collect-coverage && make-coverage-badge",
    "skip-ci": "git commit --allow-empty -m \"chore(root): [skip ci]\""
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@babel/core": "7.21.3",
    "global": "^4.4.0",
    "patch-package": "6.5.1",
    "typescript": "5.0.2",
    "yalc": "^1.0.0-pre.53"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@commitlint/cli": "17.5.0",
    "@commitlint/config-conventional": "17.4.4",
    "@commitlint/prompt-cli": "17.5.0",
    "@preconstruct/cli": "2.4.0",
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "chalk": "5.2.0",
    "commit-and-tag-version": "11.2.0",
    "css-mediaquery": "0.1.2",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-chain": "1.1.6",
    "jest-environment-jsdom": "29.5.0",
    "jest-extended": "3.2.4",
    "lint-staged": "13.2.0",
    "make-coverage-badge": "1.2.0",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.8.6",
    "ts-node": "10.9.1",
    "typedoc": "0.23.28",
    "typedoc-plugin-markdown": "3.14.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "resolutions": {
    "@types/react": "18.0.9"
  },
  "preconstruct": {
    "packages": [
      "packages/*"
    ]
  }
}
